% TODO: There should be an extra clipping for only the remaining coordinates
\newcommand{\partialBoardClipping}[2]{
  \calculateStep{#1}{#2}

  \clip (-1 * \step, - 1 * \step) rectangle 
      + (3 * \step,  3 * \step);
}

% Parameters
%
% 1: dimension (in cm)
% 2: board size (square)
% 
% Example: A 19x19 board with size 10cm x 10cm: `\goban{10}{19}`
\newenvironment{goban}[1][]{
  \begin{tikzpicture}[scale = 1, transform shape]
    \pgfkeys{/phili/goGrid/.cd, #1}
    
    \goGrid
}{
    \setcounter{moveCounter}{1}
  \end{tikzpicture}
}

