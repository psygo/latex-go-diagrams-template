dig_w := 0.8dig_w;
dig_pen := 0.9dig_pen;

dig_w := .7dig_w;
dig_h := .9dig_h;
dig_pen := .8dig_pen;

digit_zero; digit_one; digit_two; digit_three; digit_four;
digit_five; digit_six; digit_seven; digit_eight; digit_nine;

